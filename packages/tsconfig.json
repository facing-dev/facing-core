{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "noUnusedLocals": false
  },
}